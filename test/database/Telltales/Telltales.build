<?xml version="1.1" encoding="UTF-8"?>
<build:BuildSettings xmlns:build="http://www.populus.luxoft.com/build">
  <BuildOptions>
    <SerializeDDH>true</SerializeDDH>
    <SerializeFontsAndBitmaps>true</SerializeFontsAndBitmaps>
    <SerializeLanguages>false</SerializeLanguages>
    <GenerateCharUsage>false</GenerateCharUsage>
    <GenerateNameToIDMap>false</GenerateNameToIDMap>
    <GenerateSerializationLog>false</GenerateSerializationLog>
    <GenerateObjectUsage>false</GenerateObjectUsage>
    <CopyReportBitmaps>false</CopyReportBitmaps>
    <EmbedFontAndImageInfo>true</EmbedFontAndImageInfo>
    <CompileScripts>false</CompileScripts>
    <ClearOutputDirectory>true</ClearOutputDirectory>
  </BuildOptions>
  <Languages>
    <LanguageName>English</LanguageName>
    <LanguageFormat>UTF8</LanguageFormat>
    <IncludeInSerialization>false</IncludeInSerialization>
  </Languages>
  <Languages>
    <LanguageName>Swedish</LanguageName>
    <LanguageFormat>UTF8</LanguageFormat>
    <IncludeInSerialization>false</IncludeInSerialization>
  </Languages>
  <PlatformOptions>
    <DefaultImagePreprocessMode>All</DefaultImagePreprocessMode>
    <SerializeMostSignificantByteOrder>false</SerializeMostSignificantByteOrder>
    <ImagePreprocessor>
      <Path>ImagePreprocessorPlugin\imageConverterPlugin.exe</Path>
      <CmdLineOptions>--processing=off --output-format=32 --pixel-layout=bgr</CmdLineOptions>
    </ImagePreprocessor>
  </PlatformOptions>
  <ImageGroupList>
    <ImageGroup>
      <Name>Palette</Name>
      <isTextureAtlas>false</isTextureAtlas>
      <CommandLineArguments>--processing=palette --output-format=32 --bitdepth=2 --align=1</CommandLineArguments>
      <ImagePath>\Telltales\Bitmaps\Parking_Break_palette.png</ImagePath>
      <ImagePath>\Telltales\Bitmaps\Airbag_Warning_palette.png</ImagePath>
      <ImagePath>\Telltales\Bitmaps\palette.png</ImagePath>
    </ImageGroup>
  </ImageGroupList>
</build:BuildSettings>